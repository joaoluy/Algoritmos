#include <iostream>
#include <vector>
 
using namespace std;
 
int main() {
    int a[3] = {0};
    int i,n;
    

    for(i=0; i<3; i++){
        cin >> a[i];
    }

    for(i=0; i<3; i++){
        if(a[i]<=10){
            cout << "A[" << i <<"] = " << a[i] << endl;
        }
    }
    return 0;
}
